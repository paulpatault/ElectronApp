{
  "name": "electron-todo-app",
  "productName": "Todo app",
  "author": {
    "name": "Paul Patault",
    "email": "p.patault@gmail.com"
  },
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=./build/electron.icns --prune=true --out=release-builds",
    "create-installer-mac": "electron-installer-dmg ./release-builds/Todo\\ app-darwin-x64/Todo\\ app.app electron-todo-app --out=release-builds --overwrite --icon=./build/electron.icns"
  },
  "license": "MIT",
  "devDependencies": {
    "electron": "^1.8.8",
    "electron-builder": "^22.4.1",
    "electron-installer-dmg": "^3.0.0",
    "electron-packager": "^14.2.1"
  },
  "dependencies": {
    "fs": "0.0.1-security",
    "jquery": "^3.4.1",
    "mousetrap": "^1.6.5",
    "popper": "^1.0.1",
    "python-shell": "^1.0.7",
    "xmlhttprequest": "^1.8.0"
  }
}